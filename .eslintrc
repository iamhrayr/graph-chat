{
    "extends": ["airbnb-base"],
    "env": {
        "node": true,
        "mocha": true,
        "es6": true        
    },
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": 0,
        "global-require": 0,
        "arrow-body-style": 0,
        "arrow-parens": 0,
        "no-underscore-dangle": 0,
        "import/no-dynamic-require": 0,
        "eslint linebreak-style": [0, "error", "windows"]
    }
}

